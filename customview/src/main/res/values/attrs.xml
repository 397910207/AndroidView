<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView2">
        <attr name="imageSrc" format="reference"/>
        <attr name="inSampleSize" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BrokenLineView2">
        <attr name="linePaintColor" format="color" />
        <attr name="textPaintColor" format="color" />
        <attr name="textPaintSize" format="dimension" />
        <attr name="circlePaintColor" format="color" />
        <attr name="circleRadius" format="dimension" />
        <attr name="textVerticesPaintColor" format="color" />
        <attr name="textVerticesPaintSize" format="dimension" />
        <attr name="brokenLinePaintColor" format="color" />
        <attr name="lineWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="waveLength" format="dimension"/>
        <attr name="rise" format="boolean"/>
        <attr name="boatBitmap" format="reference"/>
        <attr name="originY" format="dimension"/>
        <attr name="waveHeight" format="dimension"/>
        <attr name="duration" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="pickerview_textSize" format="dimension"/>
        <attr name="pickerview_textColorOut" format="color"/>
        <attr name="pickerview_textColorCenter" format="color"/>
        <attr name="pickerview_dividerColor" format="color"/>
        <attr name="pickerview_lineSpacingMultiplier" format="float"/>
    </declare-styleable>

    <declare-styleable name="wheelview">
        <attr name="wheelview_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="wheelview_textSize" format="dimension"/>
        <attr name="wheelview_textColorOut" format="color"/>
        <attr name="wheelview_textColorCenter" format="color"/>
        <attr name="wheelview_dividerColor" format="color"/>
        <attr name="wheelview_lineSpacingMultiplier" format="float"/>
    </declare-styleable>

    <declare-styleable name="ScrollText">
        <attr name="clickEnable" format="boolean" />
        <attr name="isHorizontal" format="boolean" />
        <attr name="speed" format="integer" />
        <attr name="text" format="string" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
        <attr name="times" format="integer" />
    </declare-styleable>

    <declare-styleable name="DYLoadingView">
        <attr name="radius1" format="dimension"/>
        <attr name="radius2" format="dimension"/>
        <attr name="gap" format="dimension"/>
        <attr name="rtlScale" format="float"/>
        <attr name="ltrScale" format="float"/>
        <attr name="color1" format="color"/>
        <attr name="color2" format="color"/>
        <attr name="mixColor" format="color"/>
        <attr name="duration_dy" format="integer"/>
        <attr name="pauseDuration" format="integer"/>
        <attr name="scaleStartFraction" format="float"/>
        <attr name="scaleEndFraction" format="float"/>
    </declare-styleable>

    <declare-styleable name="PureVerticalSeekBar">
        <attr name="vertical_color" format="color" />
        <attr name="circle_radius" format="dimension" />
        <attr name="circle_color" format="color" />
        <attr name="dragable" format="boolean" />
        <attr name="image_thumb" format="reference" />
        <attr name="image_progressbar" format="reference" />
        <attr name="max_progress" format="integer"/>
        <attr name="min_progress" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RopeProgressBar">
        <attr name="ropeMax" format="integer"/>
        <attr name="ropeProgress" format="integer"/>
        <attr name="ropePrimaryColor" format="color"/>
        <attr name="ropeSecondaryColor" format="color"/>
        <attr name="ropeSlack" format="dimension"/>
        <attr name="ropeStrokeWidth" format="dimension"/>
        <attr name="ropeDynamicLayout" format="boolean"/>
    </declare-styleable>

    <attr name="mwhWaves" format="string|enum">
        <enum name="MultiWave" value="-1"/>
        <enum name="PairWave" value="-2"/>
    </attr>
    <declare-styleable name="MultiWaveHeader">
        <attr name="mwhWaveHeight" format="dimension"/>
        <attr name="mwhStartColor" format="color"/>
        <attr name="mwhCloseColor" format="color"/>
        <attr name="mwhIsRunning" format="boolean"/>
        <attr name="mwhColorAlpha" format="fraction|float"/>
        <attr name="mwhProgress" format="fraction|float"/>
        <attr name="mwhVelocity" format="fraction|float"/>
        <attr name="mwhGradientAngle" format="integer"/>
        <attr name="mwhWaves"/>
    </declare-styleable>

    <declare-styleable name="MiClockView">
        <attr name="backgroundColor" format="color" />
        <attr name="lightColor" format="color" />
        <attr name="darkColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <!--边缘透明View-->
    <declare-styleable name="EdgeTransparentView">
        <attr name="edge_position">
            <flag name="top" value="0x01"/>
            <flag name="bottom" value="0x02"/>
            <flag name="left" value="0x04"/>
            <flag name="right" value="0x08"/>
        </attr>

        <attr name="edge_width" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CircleLayout">
        <attr name="radium" format="dimension"/>
        <attr name="changeCorner" format="float"/>
    </declare-styleable>


</resources>